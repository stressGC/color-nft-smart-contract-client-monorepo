{
	"name": "nft-first-smart-contract",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"react-use": "^17.3.2",
		"web3": "^1.7.1"
	},
	"scripts": {
		"start": "run-s dev",
		"dev": "run-p _dev:*",
		"_dev:app": "react-scripts start",
		"_dev:css": "tailwindcss build -i src/styles/main.css -o src/styles/tailwind.generated.css --watch",
		"build": "run-s _build:*",
		"_build:css": "tailwindcss build -i src/styles/main.css -o src/styles/tailwind.generated.css --minify",
		"_build:app": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"migrate": "truffle migrate"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"typescript": "^4.6.2",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.4",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.1",
		"@types/node": "^16.11.26",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.13",
		"@tailwindcss/forms": "^0.5.0",
		"autoprefixer": "^10.4.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.8",
		"tailwindcss": "^3.0.23",
		"truffle": "^5.5.4",
		"type-fest": "^2.12.0"
	}
}
